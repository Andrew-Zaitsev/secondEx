{
  "name": "secondEx",
  "version": "1.0.0",
  "description": "making a search hotel numbers site",
  "main": "headers-and-footors.pug",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "hnf": "parcel headers-n-footers.pug -p 3004 --open --out-dir dist/headers-n-footers --cache-dir dist/cache",
    "fe": "parcel form-elements.pug -p 3002 --open --out-dir dist/form-elements --cache-dir dist/cache",
    "clean": "rm -rf dist/*",
    "cleanfe": "rm -rf dist/form-elements/* && npm run fe",
    "cleanhnf": "rm -rf dist/headers-n-footers/* && npm run hnf",
    "index": "parcel src_/pages/index/index.pug -p 3000 --open --out-dir dist/index --cache-dir dist/cache/index",
    "cleani": "npm run clean && npm run index"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Andrew-Zaitsev/secondEx.git"
  },
  "keywords": [],
