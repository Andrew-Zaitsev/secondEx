{
  "name": "secondEx",
  "version": "1.0.0",
  "description": "making a search hotel numbers site",
  "main": "headers-and-footors.pug",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run clean-dist && parcel src/pages/index/index.pug -p 3000 --open --out-dir dist --cache-dir cache",
    "build": "npm run clean-build && parcel build src/pages/index/index.pug --out-dir build --cache-dir cache-build --public-url ./",
    "deploy": "gh-pages -d build",
    "w-dev": "cross-env NODE_ENV=development webpack serve --mode development --open",
    "w-build": "cross-env NODE_ENV=production webpack --mode production",
    "w-deploy": "gh-pages -d w-build",
    "clean-dist": "rm -rf dist/*",
    "clean-build": "rm -rf build/*",
    "clean-caches": "rm -rf cache/* && rm -rf cache-build/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Andrew-Zaitsev/secondEx.git"
  },
  "keywords": [],
  "author": "Andrey Zaitsev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Andrew-Zaitsev/secondEx/issues"
  },
  "homepage": "https://github.com/Andrew-Zaitsev/secondEx#readme",
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "parcel-bundler": "^1.12.4",
    "pug": "^3.0.0",
    "sass": "^1.26.10"
  },
  "dependencies": {
    "air-datepicker": "^2.2.3",
    "autoprefixer": "^9.8.5",
    "browserlist": "^1.0.1",
    "caniuse-lite": "^1.0.30001107",
    "jquery": "^3.5.1",
    "src": "^1.1.2"
  }
}