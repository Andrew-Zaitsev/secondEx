  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Andrew-Zaitsev/secondEx/issues"
  },
  "homepage": "https://github.com/Andrew-Zaitsev/secondEx#readme",
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "pug": "^3.0.0",
    "sass": "^1.26.10"
  },
  "dependencies": {
    "air-datepicker": "^2.2.3",
    "autoprefixer": "^9.8.5",
    "browserlist": "^1.0.1",
    "caniuse-lite": "^1.0.30001107",
    "jquery": "^3.5.1",
    "src": "^1.1.2"
  }
}